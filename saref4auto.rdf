    <rdfs:range rdf:resource="https://saref.etsi.org/core/Property"/>
    <rdfs:domain rdf:resource="https://saref.etsi.org/core/Device"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasParkingSpotState">
    <rdfs:label xml:lang="en">hasParkingSpotState</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the state of a parking spot</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasState"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasMovement">
    <rdfs:label xml:lang="en">hasMovement</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the movement of an entity in the vehicle environment.</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/hasProperty"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/measurementMadeBy">
    <rdfs:label xml:lang="en">measurementMadeBy</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relation between a measurement and the device that made it.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfEquipment">
    <rdfs:label xml:lang="en">consists of equipment</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to describe the type of equipment that can compose the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/consistsOf"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfEntity">
    <rdfs:label xml:lang="en">consists Of Entity</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to describe the entities that can compose the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/consistsOf"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasDestination">
    <rdfs:label xml:lang="en">hasDestination</rdfs:label>
    <rdfs:comment>A relation that defines the destination of an automotive object. For example, in a platoon, the current platoon leader defines the platoon destination. Following vehicles will share (partially) the route of the leader and can, therefore, have different final destinations.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2006/time#hasBeginning">
    <rdfs:label xml:lang="en">hasBeginning</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2006/time</rdfs:isDefinedBy>
    <rdfs:comment>Beginning of a temporal entity</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/hasProperty">
    <rdfs:label xml:lang="en">controls property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relationship between a feature of interest and a property of that feature</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/core/Property"/>
    <rdfs:domain rdf:resource="https://saref.etsi.org/core/FeatureOfInterest"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasSize">
    <rdfs:label xml:lang="en">hasSize</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that a platoon has a size. This size is defined as 1) the total number of vehicles currently in the platoon; or 2) Length of the platoon (sum of all vehicles' length + inter-vehicle distance) via s4auto:hasLength property</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/hasProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasHeight">
    <rdfs:label xml:lang="en">hasHeight</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the height of an entity, e.g., a vehicle or an entity in the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/hasProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/makesMeasurement">
    <rdfs:label xml:lang="en">makes measurement</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relation between a device and the measurements it makes. Such measurement will link together the value of the measurement, its unit of measure and the property to which it relates.</rdfs:comment>
    <owl:inverseOf rdf:resource="https://saref.etsi.org/core/measurementMadeBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/isConfidenceOf">
    <rdfs:label xml:lang="en">isConfidenceOf</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between the confidence (s4auto:Confidence class) of an estimated measurement and this measurement (saref:Measurement class)</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonRole">
    <rdfs:label xml:lang="en">hasPlatoonRole</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to specify that in a platoon a vehicle can assume roles such as: unknown, leader, follower, ready-for-leading, trailing, etc.</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasRole"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasVehicleEnvironmentMember">
    <rdfs:label xml:lang="en">hasVehicleEnvironmentMember</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that a vehicle can have members in its environment</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasMember"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/isMemberOfVehicleEnvironment">
    <rdfs:label xml:lang="en">isMemberOfVehicleEnvironment</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that an entity, e.g., a vehicle, can be member of  another vehicle's environment</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/isMemberOf"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasState">
    <rdfs:label xml:lang="en">hasState</rdfs:label>
    <rdfs:comment xml:lang="en">An hasState relation to connect to a certain state. The saref:hasState property could not be reused because it has domain saref:Device, but a s4auto:Platoon or s4auto:Vehicle are not devices. Therefore, a new s4auto:hasState property with subroperties has been created.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonPosition">
    <rdfs:label xml:lang="en">hasPlatoonPosition</rdfs:label>
    <rdfs:comment xml:lang="en">A relatiA relation to express that a vehicle member of a platoon has a platoon position that is defined as the index of the vehicle in the platoon starting from zero (leader) up to N (trailing vehicle)</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPosition"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/consistsOf">
    <rdfs:label xml:lang="en">consists of</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core"/>
    <rdfs:comment xml:lang="en">A relationship indicating a composite entity that consists of other entities (e.g., a temperature/humidity sensor that consists of a temperature sensor and a humidity sensor)</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasDestinationEndPoint">
    <rdfs:label xml:lang="en">hasDestinationEndPoint</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the destination of a vehicle, a platoon or a route in terms of an end point with latitude and longitude</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasDestination"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToProperty">
    <rdfs:label xml:lang="en">relates to property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relationship between a measurement and the property it relates to</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredByDevice">
    <rdfs:label xml:lang="en">is measured by device</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relationship specifying the devices that can measure a certain property</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasVehicleRole">
    <rdfs:label xml:lang="en">hasVehicleRole</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between Vehicle and VehicleRole classes, to express the role the vehicle plays in traffic. If not specified, it is assumed that it takes a default value. Otherwise, possible values to be specified are: publicTransport, specialTransport, dangerousGoods, roadWork, rescue, emergency, safetyCar, agriculture, commercial, military, roadOperator, taxi (see ETSI TR 103 508).</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasRole"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/isMemberOf">
    <rdfs:label xml:lang="en">isMemberOf</rdfs:label>
    <rdfs:comment xml:lang="en">A general relationship to express that an entity is a member of another entity</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasParkingVehicleState">
    <rdfs:label xml:lang="en">hasParkingVehicleState</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the state of a vehicle that is parking</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasState"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToMeasurement">
    <rdfs:label xml:lang="en">relates to measurement</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relationship between a property and the measurements it relates to</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasRole">
    <rdfs:label xml:lang="en">hasRole</rdfs:label>
    <rdfs:comment xml:lang="en">A general relation to express that an entity can have a role</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/saref4auto/Role"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasBrakeCapacity">
    <rdfs:label xml:lang="en">hasBrakeCapacity</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that the vehicle has the capacity to reduce its speed (by action on the brake pedal or an autonomous actuator)</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/saref4auto/BrakeCapacity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasRelativePosition">
    <rdfs:label xml:lang="en">hasPlatoonPosition</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between an entity, e.g., a vehicle or road entity, and RelativePosition to represent its relative position (e.g., a car is x,y,z in front of another car)</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPosition"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasEstimatedRendezvousLocation">
    <rdfs:label xml:lang="en">hasEstimatedRendezvousLocation</rdfs:label>
    <rdfs:comment>A relation to express that, during the forming state, a vehicle member of a platoon is given an estimated rendezvous location for joining the platoon. It is expressed in global coordinates (lat, long, alt), e.g. according to WGS 84.</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/saref4auto/RendezvousLocation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasIdentifier">
    <rdfs:label xml:lang="en">hasIdentifier</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between an entity and its identifier (s4auto:Identifier) which is used to identify 1) a vehicle (station) during data exchange with other devices/stations 2) a platoon during data exchange with other devices/stations, 3) a parking spot within a parking area during data exchange with other devices/stations. In all cases, this identifier may be a pseudonym. It may change over space and/or over time.</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/saref4auto/Identifier"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/usesMeasurement">
    <rdfs:label xml:lang="en">usesMeasurement</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that a device can use different types of measurements, such as speed, position of detected road objects (e.g., bicycles, vehicles). This applies in particular to the ElectronicControlUnit and TrafficLightController classes.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4syst/subSystemOf">
    <rdfs:label xml:lang="en">subSystemOf</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4syst/"/>
    <rdfs:comment xml:lang="en">Links a system to its super system. Properties of subsystems somehow contribute to the properties of the super system. The exact meaning of "contribute is defined by sub properties of s4syst:subSystemOf. Property s4syst:subSystemOf is transitive.</rdfs:comment>
    <rdfs:comment xml:lang="en">In SAREF4AUTO, a relation to express that the the ElectronicControlUnit (ECU) is a subsystem of the vehicle.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isControlledByDevice">
    <rdfs:label xml:lang="en">is controlled by device</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
    <rdfs:comment xml:lang="en">A relationship specifying the devices that can control a certain property</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasOrigin">
    <rdfs:label xml:lang="en">hasOrigin</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the origin of an entity (e.g., a vehicle or a route)  in terms of a StartPoint</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfObject">
    <rdfs:label xml:lang="en">consists of object</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to describe the objects that can compose the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/consistsOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasMember">
    <rdfs:label xml:lang="en">hasMember</rdfs:label>
    <rdfs:comment xml:lang="en">A general relationship to express that an entity can have members</rdfs:comment>
    <owl:inverseOf rdf:resource="https://saref.etsi.org/saref4auto/isMemberOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location">
    <rdfs:label xml:lang="en">location</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2003/01/geo/wgs84_pos#</rdfs:isDefinedBy>
    <rdfs:comment>The relation between something and the point, 
 or other geometrical thing in space, where it is.  For example, the realtionship between
 a radio tower and a Point with a given lat and long.
 Or a relationship between a park and its outline as a closed arc of points, or a road and
 its location as a arc (a sequence of points).
 Clearly in practice there will be limit to the accuracy of any such statement, but one would expect
 an accuracy appropriate for the size of the object and uses such as mapping .</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonVehicleState">
    <rdfs:label xml:lang="en">hasPlatoonVehicleState</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the state of a vehicle in a platoon</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasState"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonMember">
    <rdfs:label xml:lang="en">hasPlatoonMember</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that a platoon can have vehicles as its members</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasMember"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasConfidence">
    <rdfs:label xml:lang="en">hasConfidence</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between an estimated measurement (saref:Measurement class) and its confidence (s4auto:Confidence)</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/saref4auto/Confidence"/>
    <owl:inverseOf rdf:resource="https://saref.etsi.org/saref4auto/isConfidenceOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasShape">
    <rdfs:label xml:lang="en">hasShape</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the shape of an entity in the vehicle environment.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/hasProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfUser">
    <rdfs:label xml:lang="en">consists of user</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to describe the users that can compose the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/consistsOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasWidth">
    <rdfs:label xml:lang="en">hasWidth</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the width of an entity, e.g., a vehicle or an entity in the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/hasProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/isCollectionOf">
    <rdfs:label xml:lang="en">isCollectionOf</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the intermediate destination of a route (e.g., of a vehicle or a platoon) in terms of RoutePoint</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasLength">
    <rdfs:label xml:lang="en">hasLength</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the length of an entity, e.g., a vehicle or an entity in the vehicle environment or a platoon (Length of the platoon is the sum of all vehicles' length + inter-vehicle distance)</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/hasProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasRoute">
    <rdfs:label xml:lang="en">hasRoute</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the route of an entity, e.g., a vehicle or a platoon</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasAutomationLevel">
    <rdfs:label xml:lang="en">hasAutomationLevel</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to define the level of automation of a singular vehicle. According to SAE J3016, there are five levels of automation:
- Level 0: No Driving Automation.
- Level 1: Driver assistance.
- Level 2: Partial driving automation.
- Level 3: Conditional driving automation.
- Level 4: High driving automation.
- Level 5: Full Driving automation.
.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasOpeningTimeAvailability">
    <rdfs:label xml:lang="en">hasOpeningTimeAvailability</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the opening time of parking spot</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/isMemberOfPlatoon">
    <rdfs:label xml:lang="en">isMemberOfPlatoon</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express that a vehicle can be member of  a platoon</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/isMemberOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasDestinationAddress">
    <rdfs:label xml:lang="en">hasDestinationAddress</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the destination of a vehicle in terms of an Address</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasDestination"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonState">
    <rdfs:label xml:lang="en">hasPlatoonState</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the state of a platoon</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasState"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4syst/hasSubSystem">
    <rdfs:label xml:lang="en">hasSubSystem</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4syst/"/>
    <rdfs:comment xml:lang="en">Links a system to one of its sub systems</rdfs:comment>
    <rdfs:comment xml:lang="en">In SAREF4AUTO, a  relation to express that the vehicle has a subsystem called ‘ElectronicControlUnit' (ECU) that provides measurements about relevant properties, such as speed, acceleration, position, etc.</rdfs:comment>
    <owl:inverseOf rdf:resource="https://saref.etsi.org/saref4syst/subSystemOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasAbsolutePosition">
    <rdfs:label xml:lang="en">hasAbsolutePosition</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between an entity, e.g., a vehicle or road entity, and AbsolutePosition to represent its absolute position</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPosition"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfVehicle">
    <rdfs:label xml:lang="en">consists of vehicle</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to decribe the vehicles that can compose the vehicle environment</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/consistsOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2006/time#hasEnd">
    <rdfs:label xml:lang="en">hasEnd</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2006/time</rdfs:isDefinedBy>
    <rdfs:comment>End of a temporal entity.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasEnvironment">
    <rdfs:label xml:lang="en">hasEnvironment</rdfs:label>
    <rdfs:comment xml:lang="en">A relation that allows to describe the environment of a vehicle</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPerceptionState">
    <rdfs:label xml:lang="en">hasPerceptionState</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the state of a vehicle related to its environment</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/saref4auto/hasState"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPosition">
    <rdfs:label xml:lang="en">has position</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to define the position of an entity</rdfs:comment>
    <rdfs:range rdf:resource="https://saref.etsi.org/saref4auto/RelativePosition"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long">
    <rdfs:label>longitude</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2003/01/geo/wgs84_pos</rdfs:isDefinedBy>
    <rdfs:comment>The WGS84 longitude of a SpatialThing (decimal degrees).</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#alt">
    <rdfs:label>altitude</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2003/01/geo/wgs84_pos</rdfs:isDefinedBy>
    <rdfs:comment>The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasConfidenceValue">
    <rdfs:label xml:lang="en">hasConfidenceValue</rdfs:label>
    <rdfs:comment xml:lang="en">A relation between the confidence (s4auto:Confidence) and its value, which can be a discrete value (+/- 5 cm/s) or a level (e.g. 95 %)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasIDValue">
    <rdfs:label xml:lang="en">hasIDValue</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship to define the value of an indentifier</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasEstimatedJoiningTime">
    <rdfs:label xml:lang="en">hasEstimatedJoiningTime</rdfs:label>
    <rdfs:comment>A relation to express that, during the forming state, a vehicle member of a platoon is given an estimated time for joining</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasCoordinateX">
    <rdfs:label xml:lang="en">hasCoordinateX</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to define the X coordinate of a relative position</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasCoordinateY">
    <rdfs:label xml:lang="en">hasCoordinateY</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to define the Y coordinate of a relative position</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core/hasValue">
    <rdfs:label xml:lang="en">has value</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship defining the value of a certain property, e.g., energy or power. Note that, even if numeric values are expected to enable reasoning, measurement values could use other datatypes.</rdfs:comment>
    <rdfs:comment rdf:resource="https://saref.etsi.org/core"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat">
    <rdfs:label>latitude</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2003/01/geo/wgs84_pos</rdfs:isDefinedBy>
    <rdfs:comment>The WGS84 latitude of a SpatialThing (decimal degrees).</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core/hasTimestamp">
    <rdfs:label xml:lang="en">has timestamp</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core"/>
    <rdfs:comment xml:lang="en">A relationship stating the timestamp of an entity (e.g. a measurement).</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasCoordinateZ">
    <rdfs:label xml:lang="en">hasCoordinateY</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to define the Z coordinate of a relative position</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#inXSDDateTimeStamp">
    <rdfs:label xml:lang="en">inXSDDateTimeStamp</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2006/time</rdfs:isDefinedBy>
    <rdfs:comment>Position of an instant, expressed using xsd:dateTimeStamp</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2006/time#Instant"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasMaxSize">
    <rdfs:label xml:lang="en">hasMaxSize</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the max size of a platoon. Note that it is defined as a datatype property as this is a fixed/static value of the platoon that does not change over time. In contrast, the current size of the platoon is actually a measurement that may change over time and it is therefore defined as an object property (see s4auto:hasSize).</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonIndex">
    <rdfs:label xml:lang="en">hasPlatoonIndex</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to express the index of the vehicle in the platoon starting from zero (leader) up to N (trailing vehicle)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasDescriptionName">
    <rdfs:label xml:lang="en">hasDescriptionName</rdfs:label>
    <rdfs:comment xml:lang="en">A relation to define that a parking spot can have a description name</rdfs:comment>
  </owl:DatatypeProperty>
  <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/SafetyCar">
    <rdfs:label xml:lang="en">safetyCar</rdfs:label>
    <rdfs:comment xml:lang="en">A type of vehicle role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:VehicleRole>
  <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/Emergency">
    <rdfs:label xml:lang="en">emergency</rdfs:label>
    <rdfs:comment xml:lang="en">A type of vehicle role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:VehicleRole>
  <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/Agriculture">
    <rdfs:label xml:lang="en">agriculture</rdfs:label>
    <rdfs:comment xml:lang="en">A type of vehicle role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:VehicleRole>
  <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/Taxi">
    <rdfs:label xml:lang="en">taxi</rdfs:label>
    <rdfs:comment xml:lang="en">A type of vehicle role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:VehicleRole>
  <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/Rescue">
    <rdfs:label xml:lang="en">rescue</rdfs:label>
    <rdfs:comment xml:lang="en">A type of vehicle role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:VehicleRole>
  <s4auto:Movement rdf:about="https://saref.etsi.org/saref4auto/ReverseDirection">
    <rdfs:label xml:lang="en">reverse Direction</rdfs:label>
    <rdfs:comment xml:lang="en">A type of movement</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:Movement>
  <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
  <s4auto:PlatoonRole rdf:about="https://saref.etsi.org/saref4auto/Leader">
    <rdfs:label xml:lang="en">leader</rdfs:label>
    <rdfs:comment xml:lang="en">A type of platoon role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:PlatoonRole>
  <s4auto:PlatoonRole rdf:about="https://saref.etsi.org/saref4auto/ReadyForLeading">
    <rdfs:label xml:lang="en">ready for leading</rdfs:label>
    <rdfs:comment xml:lang="en">A type of platoon role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:PlatoonRole>
  <s4auto:Movement rdf:about="https://saref.etsi.org/saref4auto/SameDirection">
    <rdfs:label xml:lang="en">same Direction</rdfs:label>
    <rdfs:comment xml:lang="en">A type of movement</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:Movement>
  <s4auto:PlatoonRole rdf:about="https://saref.etsi.org/saref4auto/Trailing">
    <rdfs:label xml:lang="en">trailing</rdfs:label>
    <rdfs:comment xml:lang="en">A type of platoon role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:PlatoonRole>
  <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/modified"/>
  <s4auto:Movement rdf:about="https://saref.etsi.org/saref4auto/CrossingLeft">
    <rdfs:label xml:lang="en">crossing Left</rdfs:label>
    <rdfs:comment xml:lang="en">A type of movement</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:Movement>
  <s4auto:PlatoonRole rdf:about="https://saref.etsi.org/saref4auto/Follower">
    <rdfs:label xml:lang="en">follower</rdfs:label>
    <rdfs:comment xml:lang="en">A type of platoon role</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </s4auto:PlatoonRole>
  <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/DangerousGood">
