      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/AutomationLevel"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#Geometry">
