  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/ParkingSpot">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Width"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasWidth"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">ParkingSpot</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
