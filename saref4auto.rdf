    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Engaging">
    <rdfs:label xml:lang="en">engaging</rdfs:label>
    <rdfs:comment xml:lang="en">A type of state for a Vehicle in a Platoon</rdfs:comment>
