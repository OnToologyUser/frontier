        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/measuresProperty"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
