    <rdfs:comment>A spatial representation for a given feature.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
    <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/detectsPosition">
    <rdfs:label xml:lang="en">detects position</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship specifying the position (absolute or relative position) that can be detected by a certain device</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasConfidenceUnitOfMeasure">
    <rdfs:label xml:lang="en">has confidence unit of measure</rdfs:label>
    <rdfs:comment xml:lang="en">A relation that allows to specify the unit of measure associated with a certain confidence</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/measuresProperty">
    <rdfs:label xml:lang="en">measures property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core/"/>
