  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/ParkingSpotPoint">
    <rdfs:label xml:lang="en">ParkingSpotPoint</rdfs:label>
    <rdfs:comment xml:lang="en">the position of aparking spot (its centre point) in terms of lat, long and alt</rdfs:comment>
