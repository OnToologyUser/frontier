    <rdfs:comment xml:lang="en">A type of acceleration</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://saref.etsi.org/saref4auto/Acceleration"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/RoutePoint">
    <rdfs:label xml:lang="en">RoutePoint</rdfs:label>
    <rdfs:comment xml:lang="en">The intermediate points of a route</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://saref.etsi.org/saref4auto/Point"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/AtDropOffSpot">
    <rdfs:label xml:lang="en">At Drop Off Spot</rdfs:label>
    <rdfs:comment xml:lang="en">A type of state for Parking Vehicle</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/ParkingVehicleState"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/AbsoluteSpeed">
    <rdfs:label xml:lang="en">AbsoluteSpeed</rdfs:label>
    <rdfs:comment xml:lang="en">A type of speed</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Speed"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Bus">
    <rdfs:label xml:lang="en">Bus</rdfs:label>
    <rdfs:comment xml:lang="en">A type of public transport</rdfs:comment>
    <rdfs:subClassOf>
      <s4auto:VehicleRole rdf:about="https://saref.etsi.org/saref4auto/PublicTransport">
        <rdfs:label xml:lang="en">publicTransport</rdfs:label>
        <rdfs:comment xml:lang="en">A type of vehicle role</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <rdfs:label xml:lang="en">PublicTransport</rdfs:label>
        <rdfs:comment xml:lang="en">A type of vehicle. Example of public transport is a bus or a tram</rdfs:comment>
        <rdfs:subClassOf>
