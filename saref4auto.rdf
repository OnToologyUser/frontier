<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:s4syst="https://saref.etsi.org/saref4syst/"
    xmlns:s4city="https://saref.etsi.org/saref4city/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:s4auto="https://saref.etsi.org/saref4auto/"
    xmlns:saref="https://saref.etsi.org/core/"
    xmlns:sf="http://www.opengis.net/ont/sf#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:time="http://www.w3.org/2006/time#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:geosp="http://www.opengis.net/ont/geosparql#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="https://saref.etsi.org/saref4auto/">
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2021-07-26</dcterms:modified>
    <dcterms:description xml:lang="en">This ontology extends the SAREF ontology for the automotive domain. This work has been developed in the context of the STF 566, which was established with the goal to create four SAREF extensions, one of them for the automotive domain.</dcterms:description>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2020-07-27</dcterms:issued>
    <dcterms:source rdf:resource="https://saref.etsi.org/sources/saref4auto/"/>
    <dcterms:creator rdf:resource="https://www.linkedin.com/in/lauradaniele"/>
    <owl:versionInfo>v1.1.1</owl:versionInfo>
    <dcterms:publisher rdf:resource="https://www.etsi.org/"/>
    <owl:versionIRI rdf:resource="https://saref.etsi.org/saref4auto/v1.1.1/"/>
    <dcterms:contributor rdf:resource="https://scholar.google.com/citations?user=coCDIj8AAAAJ"/>
    <vann:preferredNamespaceUri>https://saref.etsi.org/saref4auto/</vann:preferredNamespaceUri>
    <rdfs:seeAlso rdf:resource="https://www.etsi.org/deliver/etsi_ts/103400_103499/10341007/01.01.01_60/ts_10341007v010101p.pdf"/>
    <vann:preferredNamespacePrefix>s4auto</vann:preferredNamespacePrefix>
    <dcterms:title xml:lang="en">SAREF extension for automotive</dcterms:title>
    <dcterms:creator rdf:resource="http://netellany.fr/"/>
    <dcterms:license rdf:resource="https://forge.etsi.org/etsi-software-license"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/AutomotiveObject">
    <rdfs:label xml:lang="en">AutomotiveObject</rdfs:label>
    <rdfs:comment xml:lang="en">The main objects of interest in the automotive domain.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/ParkingSpotState">
    <rdfs:label xml:lang="en">ParkingSpotState</rdfs:label>
    <rdfs:comment xml:lang="en">A parking spot has a state. Possible values for parking spot state are Free, occupied, reserved, closed, charging</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/State"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/CriticalObject">
    <rdfs:label xml:lang="en">CriticalObject</rdfs:label>
    <rdfs:comment xml:lang="en">A critical object located on the road. Critical objects are small object, medium object, large object</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Width"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasWidth"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Shape"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasShape"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Movement"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasMovement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Length"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasLength"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Height"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasHeight"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/StationID">
    <rdfs:label xml:lang="en">StationID</rdfs:label>
    <rdfs:comment xml:lang="en">A type of identifier for vehicles and platoons, as defined in ETSI TS 102 894-2.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Identifier"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/core/Property">
    <rdfs:label xml:lang="en">Property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core"/>
    <rdfs:comment xml:lang="en">Anything that can be sensed, measured or controlled by a device (measured by a sensor, controlled by an actuator) .</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/Measurement"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToMeasurement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/Device"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredByDevice"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/core/Device"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isControlledByDevice"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/AbsolutePosition">
    <rdfs:label xml:lang="en">AbsolutePosition</rdfs:label>
    <rdfs:comment xml:lang="en">The absolute poistion of an automotive object in terms of lat, long and lat</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#alt"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Position"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Point"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/VulnerableRoadUser">
    <rdfs:label xml:lang="en">VulnerableRoadUser</rdfs:label>
    <rdfs:comment>Non-motorized road users as well as users of VRU vehicles (L class of vehicles, for example mopeds or motorcycles, etc.) participating to the road traffic</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Width"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasWidth"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Movement"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasMovement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Length"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasLength"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Height"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasHeight"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/RelativeSpeed">
    <rdfs:label xml:lang="en">RelativeSpeed</rdfs:label>
    <rdfs:comment xml:lang="en">A type of speed</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Speed"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/ElectricChargingParkingSpot">
    <rdfs:label xml:lang="en">ElectricChargingParkingSpot</rdfs:label>
    <rdfs:comment xml:lang="en">A type pf parking spot</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/ParkingSpot"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/NotifiedEvent">
    <rdfs:label xml:lang="en">NotifiedEvent</rdfs:label>
    <rdfs:comment xml:lang="en">The relevant types of events are trafficCondition, accident, roadworks, impassability, adverseWeatherCondition-Adhesion, aquaplannning, hazardousLocation-SurfaceCondition, hazardousLocation-ObstacleOnTheRoad, hazardousLocation-AnimalOnTheRoad, humanPresenceOnTheRoad, wrongWayDriving, rescueAndRecoveryWorkInProgress, adverseWeatherCondition-ExtremeWeatherCondition, adverseWeatherCondition-Visibility, adverseWeatherCondition-Precipitation, slowVehicle, dangerousEndOfQueue, vehicleBreakdown, postCrash, humanProblem, stationaryVehicle, emergencyVehicleApproaching, hazardousLocation-DangerousCurve, collisionRisk, signalViolation, dangerousSituation</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Car">
    <rdfs:label xml:lang="en">Car</rdfs:label>
    <rdfs:comment xml:lang="en">A type of vehicle. Example of a car is a passenger car</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Vehicle"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Platoon">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasMaxSize"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Length"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasLength"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Route"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasRoute"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Size"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasSize"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/PlatoonState"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonState"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/EndPoint"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasDestinationEndPoint"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Platoon</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Vehicle"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonMember"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment>A group of vehicles automatically following each other at a relatively close distance</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Identifier"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/hasIdentifier"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Acceleration">
    <rdfs:label xml:lang="en">Acceleration</rdfs:label>
    <rdfs:comment xml:lang="en">A Property of interest for the automotive domain</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/Property"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/2006/time#TemporalEntity">
    <rdfs:label xml:lang="en">TemporaryEntity</rdfs:label>
    <rdfs:isDefinedBy>http://www.w3.org/2006/time</rdfs:isDefinedBy>
    <rdfs:comment>A temporal interval or instant.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/HeightUnit">
    <rdfs:label xml:lang="en">height unit</rdfs:label>
    <rdfs:comment xml:lang="en">unit of measure of height</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/UnitOfMeasure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/PlatoonPosition">
    <rdfs:label xml:lang="en">PlatoonPosition</rdfs:label>
    <rdfs:comment xml:lang="en">A vehicle member of a platoon has a platoon position that is defined as the index of the vehicle in the platoon starting from zero (leader) up to N (trailing vehicle).</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasPlatoonIndex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Position"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#Feature">
    <rdfs:label xml:lang="en">Feature</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql"/>
    <rdfs:comment>This class represents the top-level feature type. This class is 
      equivalent to GFI_Feature defined in ISO 19156:2011, and it is 
      superclass of all feature types.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#Geometry"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#SpatialObject"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/VehicleEnvironment">
    <rdfs:label xml:lang="en">VehicleEnvironment</rdfs:label>
    <rdfs:comment xml:lang="en">The vehicle environment is made of entities which are present on the road in the area around the vehicle</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Vehicle"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfVehicle"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="https://saref.etsi.org/saref4auto/RoadEntity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4auto/consistsOfEntity"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/P1Pedestrian">
    <rdfs:label xml:lang="en">P1 Pedestrian</rdfs:label>
    <rdfs:comment xml:lang="en">VRU Profile 1 users, for example: adult, child, elderly person, pram, animal, blind person guided by a dog, rider off its bike</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://saref.etsi.org/saref4auto/VulnerableRoadUser"/>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Platooning">
    <rdfs:label xml:lang="en">platooning</rdfs:label>
    <rdfs:comment xml:lang="en">A type of state for Platoon or for a Vehicle in a Platoon</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/PlatoonVehicleState"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/saref4auto/PlatoonState"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/Address">
    <rdfs:label xml:lang="en">Address</rdfs:label>
    <rdfs:comment xml:lang="en">A class to define common information related to an address. It is out of scope of SAREF4AUTO to define the details of such an address. It is instead recommended to reuse existing ontologies that specify how to model an address in terms of Street, Postal Code, City, Region, Country, etc.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://saref.etsi.org/saref4auto/RelativePosition">
    <rdfs:label xml:lang="en">RelativePosition</rdfs:label>
    <rdfs:comment xml:lang="en">It represents the relative position of a vehicle (e.g., a car is x,y,z in front of another car)</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasCoordinateZ"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4auto/hasCoordinateY"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
